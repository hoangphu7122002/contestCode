        "from math import factorial as f\n",
        "\n",
        "def Lattice_Paths(n,m):\n",
        "  res = f(n+m)//(f(n)*f(m)) # (n+m)C(n) or (n+m)C(m)\n",
        "  return res%(10**9+7)\n",
        "\n",
        "if __name__ == '__main__':\n",
        "  for _ in range(int(input())):\n",
        "    nm = list(map(int,input().split()))\n",
        "    n,m = nm\n",
        "    print(Lattice_Paths(n,m))"
      ],
